            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:text="Manager"
            android:layout_gravity="center"
            android:gravity="center"
            ></TextView>

    </LinearLayout>
</androidx.constraintlayout.widget.ConstraintLayout>